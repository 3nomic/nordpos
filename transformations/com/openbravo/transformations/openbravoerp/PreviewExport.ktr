    <connection>openbravoposDB</connection>
    <sql>SELECT C.SEARCHKEY as entity, T.ID as url, T.ID as uuid, T.TICKETID as id, T.TICKETTYPE as type, R.DATENEW as ticket_date,  
P.ID as cashier,
C.ID as bpid, C.NAME as customer,
L.LINE as ticketline, L.PRODUCT as product, L.UNITS as units, L.PRICE as price, 
TAX.ID as tax,
SUM(PAY.TOTAL) as total

FROM RECEIPTS R JOIN PAYMENTS PAY ON R.ID=PAY.RECEIPT JOIN TICKETS T ON R.ID = T.ID JOIN TICKETLINES L ON T.ID = L.TICKET
JOIN TAXES TAX ON L.TAXID = TAX.ID LEFT OUTER JOIN PEOPLE P ON T.PERSON = P.ID
LEFT OUTER JOIN CUSTOMERS C ON T.CUSTOMER = C.ID
WHERE (T.TICKETTYPE = 0 OR T.TICKETTYPE = 1) AND T.STATUS = 0
GROUP BY C.SEARCHKEY, T.ID, T.TICKETID, T.TICKETTYPE, R.DATENEW,P.ID, C.ID, 
C.NAME, L.LINE, L.PRODUCT, L.UNITS, L.PRICE, TAX.ID</sql>
